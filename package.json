{
  "$schema": "https://json.schemastore.org/package.json",
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/folio/issues",
  "dependencies": {
    "next": "^14.2",
    "react": "^18.3",
    "react-dom": "^18.3",
    "shuutils": "^9.1"
  },
  "description": "My personal portfolio",
  "devDependencies": {
    "@biomejs/biome": "^1.9",
    "@tsconfig/strictest": "^2.0",
    "@types/node": "^20.16",
    "@types/react": "^18.3",
    "@types/react-dom": "^18.3",
    "@vitest/coverage-v8": "^2.1",
    "eslint": "^8.57",
    "eslint-config-next": "^14.2",
    "postcss": "^8.4",
    "repo-check": "^1.37",
    "tailwindcss": "^3.4",
    "typescript": "^5.6",
    "vitest": "^2.1"
  },
  "homepage": "https://github.com/Shuunen/folio#readme",
  "keywords": [
    "HumaCode",
    "Romain Racamier-Lafon",
    "personal",
    "website",
    "page",
    "portfolio"
  ],
  "license": "GPL-3.0",
  "name": "folio",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/folio.git"
  },
  "scripts": {
    "build": "next build && pnpm build:mark && echo build success",
    "build:mark": "unique-mark .next/server/app/**/page.{js}",
    "check": "pnpm install && pnpm build && pnpm lint && pnpm test && echo check success",
    "dev": "next dev",
    "lint": "pnpm lint:biome && pnpm lint:eslint && pnpm lint:tsc && echo lint success",
    "lint:biome": "biome check --error-on-warnings --write --unsafe && echo lint:biome success",
    "lint:eslint": "next lint && echo lint:eslint success",
    "lint:tsc": "tsc --noEmit && echo lint:tsc success",
    "postversion": "git push && git push --tags",
    "preversion": "pnpm lint",
    "start": "next start",
    "test": "vitest --run --coverage && echo test success",
    "test:bench": "vitest bench",
    "test:update": "vitest --run --coverage --update && echo test update success",
    "test:watch": "vitest --coverage"
  },
  "version": "5.0.0"
}
