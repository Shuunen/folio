{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/folio/issues",
  "description": "My personal portfolio",
  "devDependencies": {
    "@icon-park/vue-next": "1",
    "@tailwindcss/typography": "0",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "@vue/eslint-config-typescript": "11",
    "autoprefixer": "10",
    "c8": "7",
    "eslint": "8.35",
    "eslint-plugin-unicorn": "45",
    "eslint-plugin-vue": "9",
    "lightgallery": "2",
    "postcss": "8",
    "repo-check": "1.28",
    "tailwindcss": "3",
    "tsm": "2",
    "typescript": "4",
    "uvu": "0",
    "vitepress": "0",
    "vue": "3",
    "watchlist": "0"
  },
  "files": [
    "public"
  ],
  "homepage": "https://github.com/Shuunen/folio#readme",
  "keywords": [
    "Romain Racamier-Lafon",
    "personal",
    "website",
    "page",
    "portfolio"
  ],
  "license": "GPL-3.0",
  "name": "folio",
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "tailwindcss": {}
    }
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/folio.git"
  },
  "scripts": {
    "build": "vitepress build pages",
    "check": "repo-check",
    "ci": "npm run build && npm run lint && npm run check && npm run test",
    "dev": "vitepress dev pages",
    "lint": "tsc --noEmit && eslint --fix --ignore-path .gitignore --ext .js,.ts,.html,.vue .",
    "postversion": "git push && git push --tags",
    "preversion": "npm run lint",
    "serve": "vitepress serve pages",
    "test": "c8 uvu -r tsm tests",
    "test:watch": "watchlist src tests -eager -- npm test"
  },
  "version": "3.0.0"
}
