  },
  "description": "My personal portfolio",
  "devDependencies": {
    "@icon-park/vue-next": "1.4",
    "@tailwindcss/typography": "0.5",
    "autoprefixer": "10",
    "dependency-cruiser": "latest",
    "eslint": "8.49",
    "eslint-config-hardcore": "41.2",
    "eslint-plugin-tailwindcss": "3",
    "postcss": "8",
    "repo-check": "1.33",
    "tailwindcss": "3",
    "typescript": "5.2",
    "vite": "*",
    "vitepress": "1.0.0-rc.14",
    "vue": "*"
  },
  "files": [
    "public"
  ],
  "homepage": "https://github.com/Shuunen/folio#readme",
  "keywords": [
    "Romain Racamier-Lafon",
    "personal",
    "website",
    "page",
    "portfolio"
  ],
  "license": "GPL-3.0",
  "name": "folio",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/folio.git"
  },
  "scripts": {
    "build": "vitepress build && echo vitepress build success && unique-mark .vitepress/dist/*.html && echo build success",
    "check": "pnpm install && repo-check && pnpm build && pnpm lint && pnpm test && echo check success",
    "dev": "vitepress dev",
    "lint": "tsc --noEmit && echo tsc success && eslint --cache --fix --ext .ts,.vue . && echo eslint success && depcruise components pages --config && echo lint success",
    "postversion": "git push && git push --tags",
    "preversion": "pnpm lint",
    "serve": "vitepress serve",
    "test": "echo no test yet"
  },
  "type": "module",
  "version": "4.0.0"
}