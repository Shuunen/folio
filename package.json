  "devDependencies": {
    "@tailwindcss/typography": "0.5",
    "autoprefixer": "10",
    "c8": "7",
    "eslint": "8.38",
    "eslint-config-hardcore": "35",
    "eslint-plugin-tailwindcss": "3",
    "postcss": "8",
    "repo-check": "1.28",
    "tailwindcss": "3",
    "tsm": "2",
    "typescript": "4",
    "uvu": "0",
