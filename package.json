{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/folio/issues",
  "description": "My personal portfolio",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5",
    "@typescript-eslint/eslint-plugin": "^5.16",
    "@typescript-eslint/parser": "^5.16",
    "autoprefixer": "^10.4",
    "c8": "^7.11",
    "eslint": "^8.11",
    "eslint-plugin-unicorn": "^43.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4",
    "repo-check": "^1.23",
    "tailwindcss": "^3.0",
    "tsm": "^2.2",
    "typescript": "^4.6",
    "uvu": "^0.5",
    "vitepress": "^0.22",
    "watchlist": "^0.3"
  },
  "files": [
    "public"
  ],
  "homepage": "https://github.com/Shuunen/folio#readme",
  "keywords": [
    "Romain Racamier-Lafon",
    "personal",
    "website",
    "page",
    "portfolio"
  ],
  "license": "GPL-3.0",
  "name": "folio",
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "tailwindcss": {}
    }
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/folio.git"
  },
  "scripts": {
    "build": "vitepress build pages",
    "check": "repo-check",
    "ci": "npm run build && npm run lint && npm run check && npm run test",
    "dev": "vitepress dev pages",
    "lint": "tsc --noEmit && eslint --fix --ignore-path .gitignore --ext .js,.ts,.html,.vue .",
    "postversion": "git push && git push --tags",
    "preversion": "npm run lint",
    "serve": "vitepress serve pages",
    "test": "c8 uvu -r tsm tests",
    "test:watch": "watchlist src tests -eager -- npm test"
  },
  "version": "3.0.0"
}
